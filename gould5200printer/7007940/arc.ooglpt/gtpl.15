	TITLE SPOOL VALRET

A=1 ? B=2 ? .RJCL=5

GO:	.BREAK 12,[.RJCL,,JCLBUF]
	MOVE A,[440700,,JCLBUF]

LOOP:	ILDB B,A
	CAIN B,^M
	 JRST DONE
	CAIE B,^@
	 JRST LOOP
DONE:	MOVEI B,^C
	DPB B,A
	MOVEI B,^V
	IDPB B,A

	.VALUE STRING

STRING:	ASCII/:KILL :QMAIL GLPT _/
JCLBUF:	BLOCK 100

	END GO

